{
  "name": "calendarlang",
  "displayName": "calendarlang",
  "description": "Syntax highlighting for calendar dsl",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "calendarlang",
      "aliases": ["calendarlang", "calendarlang"],
      "extensions": [".cal"],
